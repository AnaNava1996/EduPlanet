<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>welcome</servlet-name>
        <servlet-class>paquete.welcome</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>FileManager</servlet-name>
        <servlet-class>paquete.FileManager</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>teacher</servlet-name>
        <servlet-class>paquete.teacher</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>fail</servlet-name>
        <servlet-class>paquete.fail</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>adminservlet</servlet-name>
        <servlet-class>paquete.adminservlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Validator</servlet-name>
        <servlet-class>paquete.Validator</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>PageManager</servlet-name>
        <servlet-class>paquete.PageManager</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>UserManager</servlet-name>
        <servlet-class>paquete.UserManager</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>welcome</servlet-name>
        <url-pattern>/welcome</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>FileManager</servlet-name>
        <url-pattern>/FileManager</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>teacher</servlet-name>
        <url-pattern>/teacher</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>fail</servlet-name>
        <url-pattern>/fail</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>adminservlet</servlet-name>
        <url-pattern>/adminservlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Validator</servlet-name>
        <url-pattern>/Validator</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>PageManager</servlet-name>
        <url-pattern>/PageManager</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>UserManager</servlet-name>
        <url-pattern>/UserManager</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
